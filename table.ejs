<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabela</title>
</head>
<body>
    <table>
        <thead>
            <% header.forEach( col => { %>
                <th><%= col %></th>
            <% }) %>
        </thead>
        <tbody>
            <% rows.forEach( row => { %>
                <tr>
                    <% row.forEach( col => { %>
                        <td><%= col %></td>
                    <% }) %>
                </thead>
                </tr>
            <% }) %>
        </thead>
        </tbody>
    </table>
</body>
</html>